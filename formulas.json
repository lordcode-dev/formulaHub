  {"id":9,"name":"Mean","category":"Statistics","formula":"μ = Σx / n","description":"Average of values","inputs":["x1","x2","x3","x4","x5"],"calc":"(x1+x2+x3+x4+x5)/5"},
  {"id":10,"name":"Variance","category":"Statistics","formula":"σ² = Σ(x - μ)² / n","description":"Variance of values","inputs":["x1","x2","x3","x4","x5","mean"],"calc":"((x1-mean)**2+(x2-mean)**2+(x3-mean)**2+(x4-mean)**2+(x5-mean)**2)/5"},
