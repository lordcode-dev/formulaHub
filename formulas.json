  {"id":6,"name":"Sine Rule","category":"Trigonometry","formula":"a/sinA = b/sinB = c/sinC","description":"Relates sides and angles in triangle","inputs":["a","A","b","B","c","C"],"calc":"a/Math.sin(A) === b/Math.sin(B) && b/Math.sin(B) === c/Math.sin(C)"},
  {"id":7,"name":"Derivative of xⁿ","category":"Calculus","formula":"d/dx(xⁿ) = n*xⁿ⁻¹","description":"Derivative of a power function","inputs":["n","x"],"calc":"n*Math.pow(x,n-1)"},
