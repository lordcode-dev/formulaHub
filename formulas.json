[
  {"id":1,"name":"Quadratic Formula","category":"Algebra","formula":"x = (-b ± √(b²-4ac))/(2a)","description":"Solve ax² + bx + c = 0","inputs":["a","b","c"],"calc":"(-b + Math.sqrt(b*b-4*a*c))/(2*a)"},
  {"id":2,"name":"Slope of a Line","category":"Algebra","formula":"m = (y₂ - y₁)/(x₂ - x₁)","description":"Slope between two points","inputs":["x1","y1","x2","y2"],"calc":"(y2 - y1)/(x2 - x1)"},
  {"id":3,"name":"Area of Circle","category":"Geometry","formula":"A = πr²","description":"Area of a circle","inputs":["r"],"calc":"Math.PI*r*r"},
